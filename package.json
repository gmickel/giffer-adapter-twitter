  "author": "Gordon Mickel <gordon.mickel@gmail.com",
  "license": "ISC",
  "dependencies": {
    "inherits": "^2.0.1",
    "twitter-stream-api": "^0.4.2"
  },
  "devDependencies": {
    "tap": "^5.6.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.0",
    "eslint-plugin-react": "^4.0.0",
    "jscs": "^2.9.0",
    "npm-check-updates": "^2.5.6"
  }
}
