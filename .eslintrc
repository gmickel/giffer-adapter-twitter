{
  "extends": "airbnb",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    // in CommonJS
    "node": true
  },
  // To give you an idea how to override rule options:
  "rules": {
    "comma-dangle": 0,
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "space-before-function-paren": [0],
    "prefer-arrow-callback": [0],
    "strict": [0],
    "newline-per-chained-call": [0]
  }
}
